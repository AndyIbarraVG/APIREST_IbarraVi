{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./Parcial_2/DocumentacionJs/src"],
        "exclude": [],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",  
        "encoding": "utf8",               
        "destination": "./Parcial_2/DocumentacionJs/doc/",          
        "recurse": true
    }
}